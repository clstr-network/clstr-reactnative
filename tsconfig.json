{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@shared/*": [
        "./packages/shared/src/*"
      ],
      "@clstr/shared/*": [
        "./packages/shared/src/*"
      ]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  },
  "exclude": [
    "node_modules",
    "external",
    "external/**/*",
    "dist",
    "supabase/functions"
  ],
  "extends": "expo/tsconfig.base"
}
